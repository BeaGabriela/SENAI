// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model statuss {
  id_status  Int @id @default(autoincrement())
  status String
  s tarefasPrisma[]
}

model tarefasPrisma{
  id Int @id @default(autoincrement())
  descricao String
  horaTarefa DateTime
  horaFinalizada DateTime @default(now())

   estado statuss @relation(fields: [status], references: [id_status])
   status Int
}


